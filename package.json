    "lodash": "^4.17.15",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "react-pose": "^4.0.10",
    "react-typography": "^0.16.19",
    "semantic-ui-react": "^0.88.2",
