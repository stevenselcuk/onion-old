    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "keywords": [
    "gatsby",
    "template",
    "starter",
    "boilerplate",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"./src/**/*.{js,jsx,json,md}\"",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "storybook": "NODE_PATH=src start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix ./src",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PREFIX_LINKS__": ""
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/public/",
      "/templates/",
      "/.cache"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenselcuk/onion"
  },
  "bugs": {
    "url": "https://github.com/stevenselcuk/onion/issues"
  }
}
